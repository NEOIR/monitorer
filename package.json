{
  "name": "monitorer",
  "version": "0.0.0",
  "description": "Check, watch and monitor web services uptime.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:job\" \"yarn dev:web\"",
    "dev:job": "node ./scripts/dev/cron",
    "dev:web": "node ./src/server",
    "build": "next build ./src/web",
    "start:job": "node ./job",
    "start:web": "node ./src/server",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint .",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "@inspired-beings/log": "1.2.0",
    "axios": "0.19.0",
    "chart.js": "2.9.3",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "8.2.0",
    "koa": "2.11.0",
    "koa-router": "7.4.0",
    "next": "9.1.5",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "@socialgouv/eslint-config-react": "0.11.0",
    "@socialgouv/eslint-config-recommended": "0.11.0",
    "@types/jest": "24.0.23",
    "@types/mongodb": "3.3.12",
    "@types/next": "9.0.0",
    "@zeit/next-css": "1.0.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-emotion": "10.0.23",
    "concurrently": "5.0.1",
    "cron": "1.7.2",
    "eslint": "6.7.2",
    "jest": "24.9.0",
    "json-loader": "0.5.7",
    "mongoose": "^5.8.1",
    "prettier": "1.19.1"
  },
  "engines": {
    "node": "12",
    "npm": "6"
  }
}
